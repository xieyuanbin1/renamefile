{
  "name": "renamefile",
  "version": "1.0.0",
  "description": "rename files",
  "main": "dist/main.js",
  "bin": "dist/main.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "pkg:win": "pkg dist/* --output out-win/run --targets node16-win -C gz",
    "pkg:mac": "pkg dist/* --output out-mac/run --targets node16-macos -C gz",
    "postpkg:win": "gulp zipWin",
    "postpkg:mac": "gulp zipMac",
    "clean": "rimraf *.zip out*",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "pkg": {
    "scripts": [],
    "assets": []
  },
  "devDependencies": {
    "@types/ini": "^1.3.31",
    "@types/node": "^20.2.5",
    "gulp": "^4.0.2",
    "gulp-zip": "^5.1.0",
    "pkg": "^5.8.1",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "ini": "^4.1.1"
  }
}
